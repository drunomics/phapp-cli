{
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE.txt"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": ["Tests"],
      "in": "vendor"
    },
    {
      "name": "*.yml",
      "in": "defaults"
    },
    {
      "name": "{\\.(sh|bash|fish|zsh)}",
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "main": "phapp",
  "output": "phapp.phar",
  "stub": true
}
